<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Jumb-lo-tron 1.0</name>
    <description>
        Create jumbles of sound using up to 10 loops at a time, each looping for different lengths.
        Create soundscapes with Earth Harp and "Sonar" samples, or cacophonies with TR-707 and percussive samples. 
        Change rates to flavor your tasty jumbles. The world is your Jumble. JUMBLE.
    </description>
    <author email="psmusiceducation@gmail.com">
        Patrick Summey
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
